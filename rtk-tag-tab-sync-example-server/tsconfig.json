{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "rootDir": "./src",
    "outDir": "./dist"
  }
}
