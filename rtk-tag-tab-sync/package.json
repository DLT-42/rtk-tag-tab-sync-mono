{
  "name": "rtk-tag-tab-sync",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "Utilities supporting cross tab tag invalidation for Redux Tooklit",
  "author": "Daniel Taylor <dev.dlt.42@gmail.com>",
  "keywords": [
    "rtk",
    "tag",
    "invalidation",
    "tab"
  ],
  "scripts": {
    "test": "vitest run",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "check-exports-debug": "attw --pack . --ignore-rules=cjs-resolves-to-esm -f json",
    "clean": "rm -r dist && rm -r node_modules & rm tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.0",
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "msw": "^2.6.5",
    "node-fetch": "^3.3.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-singlefile": "^2.0.3",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.1.5"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "@reduxjs/toolkit": "^2.3.0",
    "react-dom": "^18.3.1",
    "react-new-window": "^1.0.1",
    "react-redux": "^9.1.2"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "resolutions": {
    "typescript": "5.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DLT-42/rtk-tag-tab-sync-mono.git"
  }
}
